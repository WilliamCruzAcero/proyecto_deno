<%- await include('views/header.ejs') %>

    <body onload="load()" >
        <%- await include('views/navbar.ejs') %>
        <div class="container mt-3">
            <div class="jumbotron">
                
                <h2 style="color:black mt-3;">Iniciar sesión</h2>

                <div class="form-group">
                    <label for="exampleInputEmail1" class="form-label">Email</label>
                    <input type="email" class="form-control" id="emailUsuario" aria-describedby="emailHelp"
                        name="emailUsuario" required>
                </div>

                <div class="form-group">
                    <label for="exampleInputPassword1" class="form-label">Contraseña</label>
                    <input type="password" class="form-control" id="passwordUsuario" name="passwordUsuario" required>
                </div>

                <button class="btn btn-primary mt-3 mb-5" id="login">Iniciar sesión</button>

                <button class="btn  btn-outline-secondary mt-3 mb-5" id="btn">Registrar</button>
            </div>
        </div>
        <script src="./javascript/set-navBar-token.js"></script>
        <script src="./javascript/iniciar-sesion.js"></script>

    </body>

    <%-await include('views/footer.ejs') %>